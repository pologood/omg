<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.omg.xxx.dal.dao.UserMapper">
	<resultMap id="BaseResultMap" type="com.omg.xxx.dal.model.User">
		<id column="id" property="id" jdbcType="BIGINT" />
		<result column="user_id" property="userId" jdbcType="CHAR" />
		<result column="name" property="name" jdbcType="VARCHAR" />
	</resultMap>

	<select id="selectById" resultMap="BaseResultMap" parameterType="java.lang.Long">
		select id, user_id, name
		from tb_user
		where id = #{id,jdbcType=BIGINT}
	</select>

	<insert id="insert" parameterType="com.omg.xxx.dal.model.User">
		insert into tb_user (id, user_id, name)
		values (#{id,jdbcType=BIGINT}, #{userId,jdbcType=CHAR}),#{name,jdbcType=VARCHAR})
	</insert>
	
	<delete id="deleteById" parameterType="java.lang.Long">
	    delete from tb_user where id = #{id,jdbcType=BIGINT}
  </delete>
</mapper>